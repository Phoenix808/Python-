name,extension=input("enter the file name").split(".")
if extension[-1]=="y" and extension[-2]=="p":
    print("The extension of file is:'python'")
elif extension[-1]=="p" and extension[-2]=="p" and name[-3]=="c":
    print("the extension of file is:'CPP'")
elif extension[-1]=="a" and extension[-2]=="v" and name[-3]=="a" and name[-4]=="j":
    print("the extension of file is:'JAVA'")
if extension[-1]=="c":
    print("the extension of file is:'C'")
